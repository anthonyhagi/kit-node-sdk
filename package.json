{
  "name": "@anthonyhagi/kit-node-sdk",
  "version": "0.0.0",
  "description": "An unofficial TypeScript/Node.js SDK for the KIT.com API, providing a simple and easy-to-use interface.",
  "type": "module",
  "keywords": [
    "kit.com",
    "kit",
    "v4"
  ],
  "license": "MIT",
  "homepage": "https://github.com/anthonyhagi/kit-node-sdk#readme",
  "bugs": {
    "url": "https://github.com/anthonyhagi/kit-node-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anthonyhagi/kit-node-sdk.git"
  },
  "author": "Anthony Hagidimitriou <info@anthonyhagi.com.au>",
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/headers.js",
  "types": "./dist/headers.d.ts",
  "exports": {
    ".": {
      "types": "./dist/headers.d.ts",
      "require": "./dist/headers.cjs",
      "import": "./dist/headers.js",
      "default": "./dist/headers.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "typecheck": "tsc",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@sxzz/eslint-config": "^7.0.1",
    "@types/node": "^22.15.21",
    "eslint": "^9.27.0",
    "prettier": "^3.5.3",
    "tsdown": "^0.12.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
